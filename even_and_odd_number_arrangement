# include <bits/stdc++.h>
using namespace std ;
int  input (vector<int>&v,int n){
   int x ;
   for (int i=0;i<n;i++){
      cin >> x ;
      v.push_back(x);
   }
}
bool Sapxep (int a , int b){
   int x , y;
   x = a%2 ;
   y = b % 2;
   if (x==0&&y==0){
      return a < b ;
   }
   if (x==1&&y==0){
      return true ;
   }else {
      return false ;
   }
}
int main (){
    int n ;
    cin >> n ;
    vector <int> v;
    input (v,n);
   sort (v.begin(),v.end(),Sapxep);
   for (auto it : v){
      cout << it << endl ;
   }
    system("pause");
    return 0 ;
} 
